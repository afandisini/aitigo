{
  "id":"nuxt-ts",
  "name":"Nuxt 3 + TypeScript",
  "description":"Nuxt 3 + TS + ESLint + Prettier + pnpm",
  "type":"frontend",
  "packageManager":"pnpm",
  "init":{"copy":true},
  "check":{"commands":[
    {"name":"lint","run":"pnpm lint"},
    {"name":"typecheck","run":"pnpm typecheck"},
    {"name":"test","run":"pnpm test","optional":true},
    {"name":"build","run":"pnpm build"}
  ]}
}